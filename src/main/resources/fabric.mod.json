{
  "schemaVersion": 1,
  "id": "minecraft-chat-alternative",
  "version": "${version}",
  "name": "MinecraftChatAlternative",
  "description": "Replace the in-game chat. No in-game chat, no report.",
  "authors": [
    "hurui200320"
  ],
  "contact": {
    "homepage": "https://github.com/hurui200320/MinecraftChatAlternative",
    "sources": "https://github.com/hurui200320/MinecraftChatAlternative",
    "issues": "https://github.com/hurui200320/MinecraftChatAlternative/issues"
  },
  "license": "AGPL-3.0",
  "icon": "assets/mca/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "info.skyblond.mc.mca.ClientModEntry"
      }
    ]
  },
  "mixins": [
    "MinecraftChatAlternative.mixins.json"
  ],
  "depends": {
    "fabric-language-kotlin": ">=1.8.6+kotlin.1.7.21",
    "fabricloader": ">=0.14.10",
    "fabric-api": "*",
    "minecraft": "1.19.2",
    "java": ">=17"
  }
}
